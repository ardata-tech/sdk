{
  "name": "@delta-storage/sdk",
  "version": "0.1.3",
  "license": "MIT",
  "author": "Delta Storage",
  "main": "dist/index.js",
  "module": "dist/deltasdk.esm.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "arrowParens": "always",
    "endOfLine": "auto"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/deltasdk.cjs.production.min.js",
      "limit": "4500 ms"
    },
    {
      "path": "dist/deltasdk.esm.js",
      "limit": "4500 ms"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@tsconfig/recommended": "^1.0.2",
    "@types/jest": "^29.5.3",
    "@web-std/file": "^3.0.3",
    "bufferutil": "^4.0.7",
    "dotenv": "^16.4.1",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "jest-extended": "^4.0.2",
    "size-limit": "^8.2.6",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delta-storage/sdk"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "socket.io-client": "^4.7.2",
    "utf-8-validate": "^6.0.3"
  }
}
